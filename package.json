{
  "name": "quik",
  "version": "0.4.6",
  "description": "A quick way to prototype apps with React and Babel with zero-setup.",
  "keywords": [
    "react",
    "babel",
    "webpack",
    "build",
    "bundle",
    "package",
    "prototype",
    "quik"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "del dist/ && babel -sd dist/ src/",
    "test": "ava test/**/*.js",
    "lint": "git diff HEAD --name-only --diff-filter=ACM | grep '.js$' | xargs eslint --quiet",
    "prepublish": "npm run build"
  },
  "bin": {
    "quik": "bin/quik.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:satya164/quik.git"
  },
  "author": "Satyajit Sahoo <satyajit.happy@gmail.com> (https://github.com/satya164/)",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-native-modules": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "chalk": "^1.1.3",
    "cheerio": "^0.20.0",
    "glob-expand": "^0.1.0",
    "json-loader": "^0.5.4",
    "koa": "^1.2.0",
    "koa-compose": "^2.4.0",
    "koa-logger": "^1.3.0",
    "koa-static": "^2.0.0",
    "koa-webpack-dev-middleware": "^1.2.0",
    "koa-webpack-hot-middleware": "^1.0.3",
    "lodash": "^4.7.0",
    "memory-fs": "^0.3.0",
    "ncp": "^2.0.0",
    "opn": "^4.0.1",
    "radium": "^0.17.1",
    "react": "^15.0.1",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.1",
    "redux": "^3.3.1",
    "webpack": "^2.1.0-beta.4",
    "yargs": "^4.3.2"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "del": "^2.2.0",
    "del-cli": "^0.2.0",
    "eslint": "^2.6.0",
    "eventsource": "^0.2.1",
    "mkdirp": "^0.5.1"
  },
  "ava": {
    "babel": "inherit"
  }
}
